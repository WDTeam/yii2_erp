        "kartik-v/yii2-mpdf": "^1.0",
        "kartik-v/yii2-builder": "^1.6",
        "yiisoft/yii2-redis": "^2.0",
        "pjkui/kindeditor": "dev-master",
        "crazyfd/yii2-qiniu": "dev-master",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-tabs-x": "*",
